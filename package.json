{
  "name": "yaxh",
  "description": "基于vue的移动端ui框架",
  "version": "0.2.6",
  "author": "yaozhaoyang",
  "license": "MIT",
  "keywords": [
    "component",
    "vue",
    "vue ui",
    "iview"
  ],
  "files": [
    "src/style",
    "src/components",
    "src/mixin",
    "src/utils",
    "lib"
  ],
  "main": "./lib/yaxh.umd.min.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "yaxh": "vue-cli-service build --target lib --name yaxh ./src/components/index.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "qs": "^6.7.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "yaxhtest": "^0.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "css-loader": "^3.2.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "style-resources-loader": "^1.2.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a523926245/Yui.git"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
